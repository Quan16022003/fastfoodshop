# API URLs
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=ws://localhost:5000

# Authentication
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_JWT_SECRET=your_jwt_secret_here

# Feature flags
REACT_APP_ENABLE_GOOGLE_LOGIN=true
REACT_APP_ENABLE_FACEBOOK_LOGIN=false

# Other configs
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png

# Lưu ý: Đây là file mẫu. Vui lòng sao chép thành .env và điền các giá trị thực tế 